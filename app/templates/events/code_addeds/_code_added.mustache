<div class="timeline-item">
  <div class="media">
    <div class="pull-left">
      <div class="marker marker-blue">
        <span class="icon icon-pull-request"></span>
      </div>
    </div>

    <div class="media-body">
      <div class="media-heading omega">
        {{#actor}}
          <a href="{{url}}">{{username}}</a>
        {{/actor}}
        submitted a
        {{#deliverable}}
          <a target="_blank" href="{{url}}">pull request</a>
        {{/deliverable}}
      </div>
    </div>
  </div>
</div>
