<div class="timeline-item">
  <div class="media">
    <div class="pull-left">
      <div class="marker marker-purple">
        <span class="icon icon-chevron-down"></span>
      </div>
    </div>

    <div class="media-body">
      <div class="media-heading omega">
        Demoted by
        {{#actor}}
          <a href="{{url}}">{{username}}</a>
        {{/actor}}

        <div class="markdown-normalized">{{{body_html}}}</div>
      </div>
    </div>
  </div>
</div>
