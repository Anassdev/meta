<% if @user_bounties && @user_bounties.present? %>
  <%= initial_store_data(
    'UserBountiesStore',
    {
      lockedBounties: ActiveModel::ArraySerializer.new(@user_bounties[:lockedBounties], each_serializer: BountySerializer),
      reviewingBounties: ActiveModel::ArraySerializer.new(@user_bounties[:reviewingBounties], each_serializer: BountySerializer),
    }.as_json
  ) %>
<% end %>
