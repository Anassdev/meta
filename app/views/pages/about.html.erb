  (function($) {
    var $chart = $('.circle-stats .chart');
    //
    $chart.easyPieChart({
      size: '90',
      lineWidth: '10',
      trackColor: '#e5e7ea',
      barColor: '#33d6a6',
      easing: 'easeOutBounce',
      scaleColor: false,
      scaleLength: false,
      onStep: function(from, to, percent) {
        $(this.el).find('.percent').text(Math.round(percent));
      }
    });
  })(jQuery);

  // -- full slider
  (function($) {
    var $fullSlider = $('.full-slider-about'),
        $fullSliderCarousel = $fullSlider.find('.carousel');
    //
    $fullSliderCarousel.owlCarousel({
      singleItem: true,
      slideSpeed: 300,
      paginationSpeed: 400,
      navigation: false,
      afterInit: afterOWLinit,
      afterUpdate: afterOWLinit
    });
    function afterOWLinit() {
      $('.owl-controls .owl-page span').append('<a class="item-link" href="#"/>');
      var pafinatorsLink = $('.owl-controls .item-link');
      //
      $.each(this.owl.userItems, function (i) {
        $(pafinatorsLink[i])
          .css({
            'background': 'url(' + $(this).find('img').attr('src') + ') center center no-repeat',
            '-webkit-background-size': 'cover',
            '-moz-background-size': 'cover',
            '-o-background-size': 'cover',
            'background-size': 'cover'
          })
          .on('click', function(e) {
            e.preventDefault();
          });
      });
    }
  })(jQuery);

  // -- quote slider
  (function($) {
    var $quoteSlider = $('.quote-slider-about'),
        $quoteSliderCarousel = $quoteSlider.find('.carousel');
    //
    $quoteSliderCarousel.owlCarousel({
      singleItem: true,
      autoPlay: true,
      slideSpeed: 300,
      paginationSpeed: 400,
      navigation: false
    });
  })(jQuery);

</script>
